<?xml version="1.0" encoding="UTF-8"?>
<datastore>
  <model name="MeterReading" package="org.e4s.model.dynamic" implements="Timestamped" timestampField="reportedTs">
    <field name="reportedTs" type="long"/>
    <field name="voltage" type="double"/>
    <field name="current" type="double"/>
    <field name="power" type="double"/>
  </model>

  <model name="WaterMeterReading" package="org.e4s.model.dynamic" implements="Timestamped" timestampField="readTs">
    <field name="readTs" type="long"/>
    <field name="usage" type="double"/>
  </model>
</datastore>
